package Exercicio04;

import java.util.Random;

public class GeraNumeroAleatorio {

	public static void main(String[] args) {
		
        Random aleatorio = new Random();
        
        int valor = aleatorio.nextInt(10)+1;
        
        System.out.println("Numero gerado: " + valor);

     
      

        }
        
	}
	
		

	