package Exercicio01;

public class Caixa {

	public static void main(String[] args) {
		public double calculaValorFinal(Produto produto,int quantidade){
		      double resultado=0;
		      switch (produto.tipo) {
		            case 1:
		                 resultado=(produto.valorquantidade);
		                 resultado= resultado-(resultado0.10);
		            break;
		            case 2:
		              resultado=(produto.valorquantidade);
		              resultado= resultado-(resultado0.20);

		            break;
		            case 3:
		               if(quantidade>5){
		                resultado=(produto.valorquantidade);
		                resultado= resultado-(resultado0.10);
		               }
		                resultado=produto.valorquantidade;
		            break;
		            default:
		                resultado=produto.valorquantidade;
		            }
		        return resultado;
		    };
		}

